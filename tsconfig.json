{
  "extends": "astro/tsconfigs/strict",
  "include": [".astro/types.d.ts", "**/*"],
  "exclude": ["dist"],
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@assets/*": ["src/assets/*"],
      "@layouts/*": ["src/layouts/*"], 
      "@pages/*": ["src/pages/*"],
      "@i18n/*": ["src/i18n/*"]
    }
  }
}
